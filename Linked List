#include <iostream>
using namespace std;

class Node{
    public:
    int data;
    Node *next;
};
Node *head;

void Print(){
    struct Node *temp = head;
    
    while(temp != NULL){
        cout << temp->data << " ";
        temp = temp->next;
    }
}

void Insert(int x){
    Node *temp = new Node();
    temp -> data = x;
    temp->next = NULL;
    if (head == NULL){
        head = temp;
        return;
    }
    Node *temp2 = head;
    while(temp2->next != NULL){
        temp2 = temp2->next;
    }
    temp2->next = temp;
    
}
void InsertB(int x){
    Node *temp = new Node();
    temp->data = x;
    temp->next = head;
    head = temp;
}

void Insertn(int x,int n){
    Node *temp1 = new Node();
    temp1->data = x;
    temp1->next = NULL;
    
    if (n==1){
        temp1->next = head;
        head = temp1;
        return;
    }
    Node *temp2 = head;
    for (int i =0;i<n-2;i++){
        temp2 = temp2->next;
    }
    temp1->next = temp2->next;
    temp2->next = temp1;
}

int main() {
	head = NULL;
	
	Insert(1);
	Insert(2);
	Insert(3);
	InsertB(4);
	InsertB(5);
	Insertn(9,1);
	Insertn(8,6);
	Print();
	return 0;
}
